{
    "name": "ART Forest Light",
    "type": "light",
    "colors": {
        "editor.foreground": "#5c6a72",
        "editor.background": "#fdf6e3",
        "editorCursor.foreground": "#5c6a72",
        "editor.selectionBackground": "#e6e2cca0",
        "editor.lineHighlightBackground": "#efebd470",
        "editorWhitespace.foreground": "#e6e2cc",
        "editorIndentGuide.background": "#87968620",
        "editorIndentGuide.activeBackground": "#87968650"
    },
    "tokenColors": [
        {
            "scope": "comment",
            "settings": {
                "foreground": "#939f91",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#dfa000"
            }
        },
        {
            "scope": "constant.numeric",
            "settings": {
                "foreground": "#df69ba"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#f85552"
            }
        },
        {
            "scope": "entity.name.function",
            "settings": {
                "foreground": "#8da101"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#3a94c5"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#35a77c"
            }
        },
        {
            "scope": "meta.embedded.java.art",
            "settings": {
                "foreground": "#e5c07b",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "source.java keyword.control",
            "settings": {
                "foreground": "#df69ba"
            }
        },
        {
            "scope": "source.java storage.type",
            "settings": {
                "foreground": "#dfa000"
            }
        },
        {
            "scope": "source.java support.function",
            "settings": {
                "foreground": "#35a77c"
            }
        }
    ]
}
